QTreeView,
QListView,
QTableView,
QColumnView {
  border-style: solid;
  border-width: 2px;
  border-color: #242424;
  background-color: #242424;
  color: #F0F0F0;
  border-radius: 0px;
}



QTreeView:disabled,
QListView:disabled,
QTableView:disabled,
QColumnView:disabled {
  background-color: #303030;
  color: #505050;
}

QTreeView:selected,
QListView:selected,
QTableView:selected,
QColumnView:selected {
  background: #242424;
  color: #F0F0F0;
}

QTreeView::hover,
QListView::hover,
QTableView::hover,
QColumnView::hover {
  background-color: #242424;
  border: 1px solid #4c4c4c;
}

QTreeView::item:pressed,
QListView::item:pressed,
QTableView::item:pressed,
QColumnView::item:pressed {
  background-color: #ff8d1c;
  border-style: solid;
  border-color: #242424;
  border-radius: 0px;
}

QTreeView::item:selected:hover,
QListView::item:selected:hover,
QTableView::item:selected:hover,
QColumnView::item:selected:hover {
  background: #ff8d1c;
  color: #000000;
  background-color: #4b3e32;
  border-style: solid;
  border-color: #242424;
  border-radius: 0px;
}

QTreeView::item:selected:active,
QListView::item:selected:active,
QTableView::item:selected:active,
QColumnView::item:selected:active {
  background-color: #ff8d1c;
  border-style: solid;
  border-color: #242424;
  border-radius: 0px;
}

QTreeView::item:!selected:hover,
QListView::item:!selected:hover,
QTableView::item:!selected:hover,
QColumnView::item:!selected:hover {
  background-color: #4b3e32;
  border-style: solid;
  border-color: #242424;
  color: #F0F0F0;
  border-radius: 0px;
}

QTableCornerButton::section {
  background-color: #303030;
  border: 1px transparent #4c4c4c;
  border-radius: 0px;
}

QTreeView[reference=true],
QListView[reference=true],
QTableView[reference=true],
QColumnView[reference=true] {
  border-style: solid;
  border-width: 2px;
  border-color: cyan;
}

QTreeView[reference_pyside=true],
QListView[reference_pyside=true],
QTableView[reference_pyside=true],
QColumnView[reference_pyside=true] {
  background-color: rgb(40,80,80);
}

/* QHeaderView ------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qheaderview

--------------------------------------------------------------------------- */
QHeaderView {
  background-color: #346e7c;
  border: 0px transparent #4c4c4c;
  padding: 0px;
  margin: 0px;
  border-radius: 0px;
}

QHeaderView:disabled {
  background-color: #303030;
  border: 1px solid #404040;
  color: #505050;
  padding: 2px;
}

QHeaderView::section {
  background-color: #346e7c;
  color: #F0F0F0;
  padding: 2px;
  border-radius: 0px;
  text-align: left;
}

QHeaderView::section:checked {
  color: #F0F0F0;
  background-color: #1464A0;
}

QHeaderView::section:checked:disabled {
  color: #787878;
  background-color: #14506E;
}

QHeaderView::section::horizontal {
  border-left: 1px solid #4c4c4c;
}

QHeaderView::section::horizontal::first, QHeaderView::section::horizontal::only-one {
  border-left: 1px solid #4c4c4c;
}

QHeaderView::section::horizontal:disabled {
  color: #787878;
}

QHeaderView::section::vertical {
  border-top: 1px solid #4c4c4c;
}

QHeaderView::section::vertical::first, QHeaderView::section::vertical::only-one {
  border-top: 1px solid #4c4c4c;
}

QHeaderView::section::vertical:disabled {
  color: #787878;
}

QHeaderView::down-arrow {
  background-color: #346e7c;
  height: 12px;
  width: 12px;
  image: url("rc:arrow_down.png");
}

QHeaderView::up-arrow {
  background-color: #346e7c;
  height: 12px;
  width: 12px;
  image: url("rc:arrow_up.png");
}


