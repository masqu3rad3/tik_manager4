QComboBox[menuButton=true] {
  min-width: 2;
  min-height: 48;
}

/* QSlider ----------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qslider

--------------------------------------------------------------------------- */
QSlider:disabled {
  background: #303030;
}

QSlider:focus {
  border: none;
}

QSlider::groove:horizontal {
  background: #c88c51;
  border: 1px solid #c88c51;
  height: 4px;
  margin: 0px;
  border-radius: 4px;
}

QSlider::groove:vertical {
  background: #c88c51;
  border: 1px solid #c88c51;
  width: 4px;
  margin: 0px;
  border-radius: 4px;
}

QSlider::add-page:vertical {
  background: #ff8d1c;
  border: 1px solid #c88c51;
  width: 4px;
  margin: 0px;
  border-radius: 4px;
}

QSlider::add-page:vertical :disabled {
  background: #c88c51;
}

QSlider::sub-page:horizontal {
  background: #fe7e00;
  border: 1px solid #c88c51;
  height: 4px;
  margin: 0px;
  border-radius: 4px;
}

QSlider::sub-page:horizontal:disabled {
  background: #c88c51;
}

QSlider::handle:horizontal {
  background: #787878;
  border: 1px solid #c88c51;
  width: 8px;
  height: 8px;
  margin: -8px 0px;
  border-radius: 4px;
}

QSlider::handle:horizontal:hover {
  background: #ff8d1c;
  border: 1px solid #ff8d1c;
}

QSlider::handle:vertical {
  background: #787878;
  border: 1px solid #c88c51;
  width: 8px;
  height: 8px;
  margin: 0 -8px;
  border-radius: 4px;
}

QSlider::handle:vertical:hover {
  background: #ff8d1c;
  border: 1px solid #148CD2;
}

