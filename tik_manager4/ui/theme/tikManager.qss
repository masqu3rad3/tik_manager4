/* QToolBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtoolbox

--------------------------------------------------------------------------- */
QToolBox {
  padding: 0px;
  border: 0px;
  border: 1px solid #4c4c4c;
}

QToolBox::selected {
  padding: 0px;
  border: 2px solid #4c4c4c;
}

QToolBox::tab {
  background-color: #303030;
  border: 1px solid #4c4c4c;
  color: #F0F0F0;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}

QToolBox::tab:disabled {
  background-color: #303030;
  color: #505050;
}

QToolBox::tab:selected {
  background-color: #4c4c4c;
  border-bottom: 2px solid #4c4c4c;
}

QToolBox::tab:selected:disabled {
  background-color: #303030;
  color: #505050;
}

QToolBox::tab:!selected {
  background-color: #c88c51;
  border-bottom: 2px solid #4c4c4c;
}

QToolBox::tab:!selected:disabled {
  background-color: #303030;
  color: #505050;
}

QToolBox::tab:hover {
  border-color: #fe7e00;
  border-bottom: 2px solid #4c4c4c;
}

QToolBox QScrollArea QWidget QWidget {
  padding: 0px;
  border: 0px;
  background-color: #303030;
}

/* QFrame -----------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qframe

--------------------------------------------------------------------------- */


.QFrame {
  border-radius: 0px;
  border: 0px solid #303030;
}

.QFrame[frameShape="0"] {
  border-radius: 0px;
  border: 0px transparent #4c4c4c;
}

.QFrame[height="3"], .QFrame[width="3"] {
  background-color: #303030;
}



/* QSplitter --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qsplitter

--------------------------------------------------------------------------- */

QSplitter {
  background-color: #303030;
  spacing: 0px;
  padding: 0px;
  margin: 0px;
}

QSplitter::separator {
  background-color: #303030;
  border: 0px solid #4c4c4c;
  spacing: 0px;
  padding: 0x;
  margin: 0px;
}

QSplitter::handle[horizontal="true"] {
  image: url("rc:splitter_horizontal.png");
  spacing: 0px;
  margin: 0px;
  padding: 0px;
}

QSplitter::handle[vertical="true"] {
  image: url("rc:splitter_vertical.png");
  spacing: 0px;
  margin: 0px;
  padding: 0px;
}

QSplitter::separator:hover {
  background-color: #787878;
}

QSplitter::separator:horizontal {
  width: 2px;
  image: url("rc:line_vertical.png");
}

QSplitter::separator:vertical {
  height: 2px;
  image: url("rc:line_horizontal.png");
}

/* QDateEdit --------------------------------------------------------------

--------------------------------------------------------------------------- */
QDateEdit {
  selection-background-color: #1464A0;
  border-style: solid;
  border: 1px solid #4c4c4c;
  border-radius: 4px;
  /* This fixes 103, 111 */
  padding-top: 2px;
  /* This fixes 103, 111 */
  padding-bottom: 2px;
  padding-left: 4px;
  padding-right: 4px;
  min-width: 10px;
}

QDateEdit:on {
  selection-background-color: #1464A0;
}

QDateEdit::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 20px;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}

QDateEdit::down-arrow {
  image: url("rc:arrow_down_disabled.png");
  height: 12px;
  width: 12px;
}

QDateEdit::down-arrow:on, QDateEdit::down-arrow:hover, QDateEdit::down-arrow:focus {
  image: url("rc:arrow_down.png");
}

QDateEdit QAbstractItemView {
  background-color: #19232D;
  border-radius: 4px;
  border: 1px solid #4c4c4c;
  selection-background-color: #1464A0;
}

QAbstractView:hover {
  border: 1px solid #4c4c4c;
  color: #F0F0F0;
}

QAbstractView:selected {
  background: #1464A0;
  color: #32414B;
}

PlotWidget {
  padding: 0px;
}

